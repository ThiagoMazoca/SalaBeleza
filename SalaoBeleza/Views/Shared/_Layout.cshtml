<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Salão de Beleza</title>
    <!-- Meta -->
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="description" content="">
    <meta name="author" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <!-- Favicon -->
    <link href="favicon.ico" rel="shortcut icon">
    <!-- Bootstrap Core CSS -->
    <link rel="stylesheet" href="~/Content/LandingPage/assets/css/bootstrap.css">
    <!-- Template CSS -->
    <link rel="stylesheet" href="~/Content/LandingPage/assets/css/animate.css">
    <link rel="stylesheet" href="~/Content/LandingPage/assets/css/font-awesome.css">
    <link rel="stylesheet" href="~/Content/LandingPage/assets/css/nexus.css">
    <link rel="stylesheet" href="~/Content/LandingPage/assets/css/responsive.css">
    <link rel="stylesheet" href="~/Content/LandingPage/assets/css/custom.css">
    <!-- Google Fonts-->
    <link rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=Open Sans:300,400" />
    <link rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=Source Sans Pro:300,400" />
</head>
<body>
    <div id="body_bg" class="container-fluid">
        <div id="container_header" class="container">
            <div id="header" class="row">
                <!-- Logo -->
                <div class="logo margin-top-15">
                    <img src="~/Content/LandingPage/assets/img/logo.png" alt="Logo" />
                </div>
                <div class="col-md-12 margin-top-15">
                    <!-- Header Social Icons -->
                    <ul class="social-icons circle pull-right">
                        <li class="social-rss">
                            <a href="#" target="_blank" title="RSS"></a>
                        </li>
                        <li class="social-twitter">
                            <a href="#" target="_blank" title="Twitter"></a>
                        </li>
                        <li class="social-facebook">
                            <a href="#" target="_blank" title="Facebook"></a>
                        </li>
                        <li class="social-googleplus">
                            <a href="#" target="_blank" title="GooglePlus"></a>
                        </li>
                    </ul>
                    <!-- End Header Social Icons -->
                </div>
                <div class="clear"></div>
            </div>
        </div>
        <div class="primary-container-group">
            <!-- Background -->
            <div class="primary-container-background">
                <div class="primary-container"></div>
                <div class="clearfix"></div>
            </div>
            <!--End Background -->
            <div id="container_hornav" class="container no-padding">
                <!-- End Logo -->
                <!-- Slogan -->
                <p class="site-slogan">Usuário Logado</p>
                <!-- End Slogan -->
                <!-- Top Menu -->
                <div class="row">
                    <div class="hornav-block">
                        <div id="hornav" class="pull-right">
                            <ul id="hornavmenu" class="nav navbar-nav">
                                <li>
                                    @Html.ActionLink("Home", "Index", "Home")
                                </li>
                                <li>
                                    @Html.ActionLink("Cabelo", "Hair", "Home")
                                </li>
                                <li>
                                    @Html.ActionLink("Maquiagem", "MakeUp", "Home")
                                </li>
                                <li>
                                    @Html.ActionLink("Manicure", "Manicure", "Home")
                                </li>
                                <li>
                                    @Html.ActionLink("Podologia", "Podiatry", "Home")
                                </li>
                                <li>
                                    @Html.ActionLink("Estética", "Aesthetics", "Home")
                                </li>
                                <li>
                                    @Html.ActionLink("Contato", "Contact", "Home")
                                </li>
                                <li>
                                    @Html.ActionLink("Login", "Index", "Home")
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>
                <!-- End Top Menu -->
            </div>
            <div class="container-fluid body-content">
                @RenderBody()
            </div>
            <div class="clearfix"></div>
        </div>
        <!-- Footer Menu -->
        <footer>
            <div class="container padding-vert-30">
                <div class="row">
                    <div id="copyright">
                        <p>(c) 2014 Your Copyright Info</p>
                    </div>
                </div>
            </div>
        </footer>
        <!-- End Footer Menu -->
    </div>
    <!-- JS -->
    <script type="text/javascript" src="~/Content/LandingPage/assets/js/jquery.min.js"></script>
    <script type="text/javascript" src="~/Content/LandingPage/assets/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Content/LandingPage/assets/js/scripts.js"></script>
    <!-- Isotope - Portfolio Sorting -->
    <script type="text/javascript" src="~/Content/LandingPage/assets/js/jquery.isotope.js"></script>
    <!-- Mobile Menu - Slicknav -->
    <script type="text/javascript" src="~/Content/LandingPage/assets/js/jquery.slicknav.js"></script>
    <!-- Animate on Scroll-->
    <script type="text/javascript" src="~/Content/LandingPage/assets/js/jquery.visible.js" charset="utf-8"></script>
    <!-- Modernizr -->
    <script src="~/Content/LandingPage/assets/js/modernizr.custom.js" type="text/javascript"></script>
    <!-- End JS -->
</body>
</html>